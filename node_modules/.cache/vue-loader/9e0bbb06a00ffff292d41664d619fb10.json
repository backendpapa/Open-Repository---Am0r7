n          <td></td>\n          <td >{{currency[\"USDEUR\"]}}</td>\n        </tr>\n         <tr\n        >\n          <td>USD<span class=\"font-weight-bold\">CHF</span></td>\n          <td></td>\n          <td></td>\n          <td></td>\n          <td></td>\n          <td></td>\n          <td >{{currency[\"USDCHF\"]}}</td>\n        </tr>\n         <tr\n        >\n          <td>USD<span class=\"font-weight-bold\">NZD</span></td>\n          <td></td>\n          <td></td>\n          <td></td>\n          <td></td>\n          <td></td>\n          <td></td>\n          <td >{{currency[\"USDNZD\"]}}</td>\n        </tr>\n        <tr\n        >\n          <td class=\"green--text font-weight-bold\">Total</td>\n          <td class=\"font-weight-bold\" >{{((currency[\"USDUSD\"]+currency[\"USDAUD\"]+currency[\"USDCAD\"]+currency[\"USDSDG\"]+currency[\"USDEUR\"]+currency[\"USDCHF\"]+currency[\"USDNZD\"])/7).toFixed(5)}}</td>\n        </tr>\n        \n      \n      </tbody>\n    </template>\n  </v-simple-table>\n    <!-- </v-row> -->\n      </v-col>\n    </v-row>\n     </v-container>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\n  export default {\n    name: 'Home',\n    data(){\n      return {\n        currency:[]\n      }\n    },\n\n    components: {\n     \n    },\n    mounted(){\n      axios.get('https://stoc3.herokuapp.com/').then(res=>{\n        this.currency=res.data.quotes\n      })\n    }\n  }\n</script>\n<style>\n.block{\n  background: url('../assets/stock.jpg'),#6DB3F2;\n  /* background: url('images/checked.png'), #6DB3F2; */\n  background-size: cover;\n  \n}\n.card {\n \n  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);\n  text-align: center;\n}\n\n\n</style>"]}]}